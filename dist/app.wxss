.animated.delay-5s {
  animation-delay: 5s;
}
.animated.fast {
  animation-duration: 800ms;
}
.animated.faster {
  animation-duration: 500ms;
}
.animated.slow {
  animation-duration: 2s;
}
.animated.slower {
  animation-duration: 3s;
}
@media (prefers-reduced-motion) {
  .animated {
    animation: unset !important;
    transition: none !important;
  }
}
page {
  font-size: 28rpx;
  line-height: 1.6;
  color: #333333;
  background-color: #F4F4F4;
}
.nav_bar {
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav_bar .nav-bar-col {
  text-align: center;
}
.nav_bar .nav-bar-col .containertitle {
  padding: 6px 10px;
  display: flex;
}
.nav_bar .nav-bar-col .containertitle .dingwei {
  height: 25px;
  display: flex;
  align-items: center;
}
.nav_bar .nav-bar-col .containertitle .dingwei text {
  font-size: 14px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.nav_bar .nav-bar-col .nav-bar-title {
  font-size: 18px;
}
.footer {
  margin: 25px 0px;
}
.footer .weui-footer__link {
  color: #007BFF;
  border: none;
  background-color: #F4F4F4;
}
.footer .weui-footer__link::after {
  border: none;
}
